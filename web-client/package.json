{
  "name": "nonce-pwa",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Dany Dodson",
  "description": "nonce-pwa",
  "private": true,
  "proxy": "http://nginx-proxy:8080",
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "serve": "serve -s -n build",
    "prod": "yarn build && yarn serve",
    "lint": "eslint './src/**/*.{js,jsx}'",
    "lint:fix": "yarn run lint --fix",
    "clean": "sudo rm -rf node_modules && sudo rm -rf build && sudo rm -rf yarn.lock",
    "refresh": "yarn clean && yarn install && yarn build",
    "sitemap": "npx sitemap-generator-cli -f './public/sitemap.xml' https://nonce.herokuapp.com/"
  },
  "dependencies": {
    "@datadog/browser-rum": "^2.5.1",
    "@material-ui/core": "^4.11.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "formik": "^2.2.6",
    "is-mobile": "^2.2.2",
    "jose": "^3.6.0",
    "notistack": "^1.0.3",
    "overmind": "^27.0.0",
    "overmind-react": "^28.0.0",
    "post": "^0.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-select": "^4.0.2",
    "react-spring": "^8.0.27",
    "request-ip": "^2.1.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "heroku": "^7.47.12",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "sitemap-generator": "^8.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}